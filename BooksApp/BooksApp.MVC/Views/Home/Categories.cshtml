@model List<Category>
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-4">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">@item.Name</h5>
                    <h6 class="card-subtitle mb-2 text-body-secondary">@item.Description</h6>
                   
                </div>
            </div>
        </div>
    }

</div>